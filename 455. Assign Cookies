class Solution {
    public int findContentChildren(int[] g, int[] s) {
        int ans = 0;
        Arrays.sort(g);
        Arrays.sort(s);
        ArrayList<Integer> cookie = new ArrayList();
        for(int i : s){
            cookie.add(i);
        }
        for(int i= 0;i<g.length;i++){
            for(int j=0;j<cookie.size();j++){
                if(cookie.get(j)>=g[i]){
                    ans++;
                    cookie.remove(j);
                    break;
                }
            }
        }
        return ans;
    }
}


Method 2
class Solution {
    public int findContentChildren(int[] g, int[] s) {
        int ans = 0;
        Arrays.sort(g);
        Arrays.sort(s);
        int i = 0;
        for(int j = 0; j<s.length&&i<g.length;j++){
            if(g[i]<=s[j]){
                i++;
            }
        }
        
        return i;
    }
}
