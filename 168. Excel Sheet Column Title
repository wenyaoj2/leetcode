class Solution {
    public String convertToTitle(int n) {
        String ans = "";
        int tail = 0;
        int as= 0;
        while(n!=0){
            tail = (n-1)%26;
            as = tail+65;
            ans = String.valueOf((char)as) + ans;
            n = (n-1)/26;
        }
        return ans;
    }
}
