class Solution {
    public char findTheDifference(String s, String t) {
        int[] ans = new int[26];
        char last = 'a';
        int num = 0;
        for(int i = 0; i<t.length()-1; i++){
            ans[t.charAt(i)-'a']++;
            ans[s.charAt(i)-'a']--;
        }
        ans[t.charAt(t.length()-1)-'a']++;
        for(int i = 0; i<ans.length;i++){
            if(ans[i]==1){
                num = i;
            }
        }
        for(int i = 0; i<t.length();i++){
            if((t.charAt(i)-'a')==num){
                last = t.charAt(i);
            }
        }
        return last;
    }
}
