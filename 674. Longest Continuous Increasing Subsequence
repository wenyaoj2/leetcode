class Solution {
    public int findLengthOfLCIS(int[] nums) {
        int length = 0;
        int start = 0;
        for(int i = 0; i<nums.length; i++){
            if(i == nums.length-1||nums[i]>=nums[i+1]){
                length = Math.max(length,i-start+1);
                start = i+1;
            }
        }
        return length;
    }
}
